<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
   <bean name="emp" class="com.my.spring.model.Employee">
   <property name="id" value="1011" />
   <property name="name" value="Rahul" />
   <property name="gender" value="male" />
   </bean>
   
   
   
   <bean name="emp2" class="com.my.spring.model.Employee">
   <constructor-arg name="id" value="2311" />
   <constructor-arg name="name" value="kunal" />
   <constructor-arg name="gender" value="male" />
   </bean>

</beans>