<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    
   <bean name="add" class="com.my.spring.model.Address">
   <property name="city" value="Noida"/>
   <property name="state" value="UP"/>
   </bean>
    
    
   <bean name="emp" class="com.my.spring.model.Employee">
   <property name="id" value="1011" />
   <property name="name" value="Rahul" />
   <property name="gender" value="male" />
   <property name="address" ref="add" />
   <property name="myList">
   <list>
   <value>4526</value>
   <value>4526</value>
   <value>4526</value>
   <value>4526</value>
   <value>4526</value>
   <value>4526</value>
   <value>4526</value>
   
   </list>
   </property>
   
   
   
   <property name="setofContacts">
   
   <set>
   <value>12</value>
   <value>11</value>
   <value>13</value>
   <value>14</value>
   <value>15</value>
   </set>
   </property>
   
   
   
   <property name="map">
   
   <map>
   <entry key="1" value="Rahul" />
   <entry key="2" value="Rohit" />
   <entry key="3" value="Aman" />
   <entry key="4" value="Deepak" />
   <entry key="5" value="Rahul" />
   </map>
   </property>
   
   
   </bean>
   
   

   <bean name="emp2" class="com.my.spring.model.Employee">
   <constructor-arg name="id" value="2311" />
   <constructor-arg name="name" value="kunal" />
   <constructor-arg name="gender" value="male" />
   <constructor-arg name="address" ref="add"/>
   <constructor-arg name="myList">
   <list>
   <value>4526</value>
   <value>4526</value>
   <value>4526</value>
   <value>4526</value>
   <value>4526</value>
   <value>4526</value>
   <value>4526</value>
   
   </list>
   </constructor-arg>
   
   
   <constructor-arg name="setofContacts">
   <set>
   <value>12</value>
   <value>11</value>
   <value>13</value>
   <value>14</value>
   <value>15</value>
   </set>
   </constructor-arg>
   
   
   
   
   <constructor-arg name="map">
    <map>
   <entry key="1" value="Rahul" />
   <entry key="2" value="Rohit" />
   <entry key="3" value="Aman" />
   <entry key="4" value="Deepak" />
   <entry key="5" value="shyam" />
   </map>
   </constructor-arg>
   
   </bean>  
   


</beans>